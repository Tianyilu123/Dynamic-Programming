//
class Solution {
    public int rob(int[] nums) {
        if(nums == null || nums.length == 0) return 0;
        int len = nums.length;
        int m1 = 0, m2 = nums[0];
        for(int i=1; i<len; i++) {
            int next = m1 + nums[i];
            m1 = Math.max(m1, m2);
            m2 = next;
        }
        return Math.max(m1, m2);
    }
}

// time: O(n)
// space: O(1)
